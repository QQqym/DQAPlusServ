  </resultMap>

  <resultMap id="BaseResultMap" type="com.cn.chinamobile.pojo.mybatis.UdpNrm" >
    <constructor >
      <arg column="NE_Type" jdbcType="VARCHAR" javaType="java.lang.String" />
